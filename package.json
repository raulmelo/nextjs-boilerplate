{
  "name": "outline-boilerplate-next",
  "version": "0.1.0",
  "private": false,
  "description": "boilerplate nextjs para iniciar um projeto base",
  "author": "Raul Melo",
  "license": "ISC",
  "scripts": {
    "dev": "(yarn tailwind-start) & next dev",
    "build": "next build",
    "export": "next export",
    "deploy": "yarn tailwind && next build && next export",
    "start": " yarn tailwind && next start",
    "generator": "yarn plop --plopfile ./.generators/propfile.js",
    "tailwind": "npx tailwindcss -c tailwind.config.js -o public/css/tailwind.css --jit --purge='./src/**/*.tsx,./src/**/*.html' --minify",
    "tailwind-start": "npx tailwindcss -c tailwind.config.js -o public/css/tailwind.css  --watch"
  },
  "dependencies": {
    "next": "10.1.3",
    "next-pwa": "^5.2.21",
    "node-sass": "^5.0.0",
    "postcss": "^8.2.14",
    "react": "17.0.2",
    "react-dom": "17.0.2"
  },
  "devDependencies": {
    "@babel/preset-typescript": "^7.12.7",
    "@storybook/addon-essentials": "6.2.9",
    "@storybook/react": "6.2.9",
    "@testing-library/jest-dom": "^5.12.0",
    "@testing-library/react": "^11.2.3",
    "@types/jest": "26.0.22",
    "@types/node": "14.14.41",
    "@types/react": "^17.0.0",
    "@typescript-eslint/eslint-plugin": "4.22.0",
    "@typescript-eslint/parser": "4.22.0",
    "babel-loader": "^8.2.2",
    "cross-env": "^7.0.3",
    "eslint": "^7.25.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-import-helpers": "^1.1.0",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-react": "^7.23.2",
    "eslint-plugin-react-hooks": "^4.2.0",
    "plop": "^2.7.4",
    "prettier": "^2.2.1",
    "typescript": "^4.2.4"
  },
  "engines": {
    "node": ">=14.16.0"
  }
}
